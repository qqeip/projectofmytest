<Events>
<Event>
<Name>On Preload</Name>
<Args/>
<Script>-- 这些操作在屏幕显示之前执行。
</Script>
</Event>
<Event>
<Name>On Back</Name>
<Args/>
<Script>-- 这些操作在点击“返回”按钮时执行。

-- 返回到上一个屏幕
Screen.Back();
</Script>
</Event>
<Event>
<Name>On Next</Name>
<Args/>
<Script>-- 这些操作在点击“下一步”按钮时执行。

-- 来自 _SUF70_Global_Functions.lua:
-- “名字:”一栏为空吗？
if(g_EditFieldIsEmpty(CTRL_EDIT_01)) then

	-- 名字栏为空...告诉用户必须填写
	-- 并停留在此屏幕 (不进入下一个屏幕)

	-- “无效的项目”
	local strTitle = SetupData.GetLocalizedString("MSG_INVALID_ENTRY");

	-- 获取“名字:”一栏的标签 (因为它可能已被翻译)
	local strFieldName = DlgStaticText.GetProperties(CTRL_STATICTEXT_LABEL_01).Text;

	-- 去除尾部的“:” (如果存在)
	strFieldName = String.TrimRight(strFieldName, ":");

	-- “&lt;fieldname&gt; 一栏不能为空。”
	local strPrompt = SetupData.GetLocalizedString("MSG_THE")
	                ..strFieldName
	                ..SetupData.GetLocalizedString("MSG_FIELD_CANNOT_BE_EMPTY");
	                
	Dialog.Message(strTitle, strPrompt, MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
else
	-- “名字:”一栏不为空...那么
	-- 进入下一个屏幕
	Screen.Next();
end
</Script>
</Event>
<Event>
<Name>On Cancel</Name>
<Args/>
<Script>-- 这些操作在点击“取消”按钮时执行。

-- 来自 _SUF70_Global_Functions.lua:
-- 询问用户是否确定要退出
if g_ConfirmSetupAbort() then
	Application.Exit();
end
</Script>
</Event>

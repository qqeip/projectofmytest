#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DBServer.exe DBServerClient.exe 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DBServer.exe: DBServer.dpr
  $(DCC)

DBServerClient.exe: DBServerClient.dpr
  $(DCC)
